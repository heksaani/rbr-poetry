<div id="<%= dom_id poet %>">
<!-- Individual poet page -->
  <h2>
    <strong>
    <%= poet.name %>
    </strong>
  </h2>
  
  <p>
  <em>Year of Birth <%= poet.year %> </em>
  </p>
  
  <p>
    <strong>Number of Poems:</strong>
    <%= poet.poems.count %>

  <p>

  <p>
  <% poet.poems.each do |poem| %>
    <%= link_to poem.name, poem %>
  <% end %>
  </p>

  <p>
    <%= link_to "All poems", poems_path %>

  </p>


</div>
